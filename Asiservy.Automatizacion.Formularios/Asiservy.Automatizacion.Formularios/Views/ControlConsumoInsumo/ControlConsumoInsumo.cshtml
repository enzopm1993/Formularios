                                    <img style="width:100%" src="~/Content/icons/add24.png" />
                                </button>
                                @if (ViewBag.LineaNegocio == "ENLATADO")
                                {
                                    <button data-toggle="tooltip" data-placement="top" title="Saldos" class="btn" id="btnModalGenerar" onclick="ModalGenerarControlDetalleSaldo()" style="border-color:#2c3e50" type="button"><img style="width:100%" src="~/Content/icons/add24-2.png" /></button>
                                }

                            </div>
                        </div>
                        <div class="col-md-9 col-sm-9 col-8">
