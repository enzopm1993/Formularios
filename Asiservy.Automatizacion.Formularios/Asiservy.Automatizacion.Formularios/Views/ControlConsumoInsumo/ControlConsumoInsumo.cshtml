<div class="modal" tabindex="-1" id="ModalGenerarControl" role="dialog" data-backdrop="static">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Control Consumo de Insumos</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <input type="hidden" id="txtIdControlConsumo" name="name" value="0" />
                    <input type="hidden" id="txtLineaNegocio" name="name" value="@ViewBag.LineaNegocio" />
                    <div class="col-md-6 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Orden Fabricación</label>
                            @*<input class="form-control" id="txtOrdenFabricacion" type="text" name="name" value="0" />*@
                            @Html.DropDownList("txtOrdenFabricacion", new SelectList(ViewBag.OrdenesFabricacion, "ORDEN_FABRICACION", "ORDEN_FABRICACION"), "seleccione", new { @class = "form-control", @Onchange = "CargarDatosOrdenFabricacion()" })
                        </div>
                    </div>

                    <div class="col-md-6 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Orden Venta</label>
                            <input class="form-control" id="txtOrdenVenta" disabled type="text" name="name" value="0" />
                        </div>
                    </div>
                    <div class="col-md-6 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Cliente</label>
                            <input class="form-control" id="txtCliente" disabled type="text" name="name" value="0" />
                        </div>
                    </div>
                    <div class="col-md-6 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Marca</label>
                            <input class="form-control" id="txtMarca" disabled type="text" name="name" value="0" />
                        </div>
                    </div>
                    @*<div class="col-md-6 col-6 col-sm-6">
                            <div hidden class="form-group">
                                <label>Hora Inicio</label>
                                <input class="form-control" id="txtHoraInicio"  type="datetime-local" name="name" value="0" />
                            </div>
                        </div>
                        <div hidden class="col-md-6 col-6 col-sm-6">
                            <div class="form-group">
                                <label>Hora Fin</label>
                                <input class="form-control" id="txtHoraFin"  type="datetime-local" name="name" value="0" />
                            </div>
                        </div>*@
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Cod Producto</label>
                            <input class="form-control" id="txtCodigoProducto" type="text" name="name" value="" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Peso Neto</label>
                            <input class="form-control" id="txtPesoNeto" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Peso Escurrido</label>
                            <input class="form-control" id="txtPesoEscrundido" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Lomo</label>
                            <input class="form-control" id="txtLomo" min="0" type="number" name="name" value="0" onkeypress=" return SoloDecimales(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Miga</label>
                            <input class="form-control" id="txtMiga" min="0" type="number" name="name" value="0" onkeypress=" return SoloDecimales(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Aceite(g)</label>
                            <input class="form-control" id="txtAceite" min="0" type="number" onkeypress=" return SoloDecimales(event,this)" name="name" value="0" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Agua(g)</label>
                            <input class="form-control" id="txtAgua" min="0" type="number" onkeypress=" return SoloDecimales(event,this)" name="name" value="0" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Caldo Vegetal(g)</label>
                            <input class="form-control" id="txtCaldoVegetal" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Desperdicio Solido</label>
                            <input class="form-control" id="txtDesperdicioSolido" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Desperdicio Liquido</label>
                            <input class="form-control" id="txtDesperdicioLiquido" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Desperdicio Aceite</label>
                            <input class="form-control" id="txtDesperdicioAceite" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Empleados</label>
                            <input class="form-control" id="txtEmpleados" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>U. Cajas</label>
                            <input class="form-control" id="txtCajas" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>U. Recibidas</label>
                            <input class="form-control" id="txtUnidadesRecibidas" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Sobrantes</label>
                            <input class="form-control" id="txtSobrantes" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>U. Producidas</label>
                            <input class="form-control" id="txtUnidadesProducidas" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                        </div>
                    </div>
                    @if (ViewBag.LineaNegocio == "ENLATADO")
                    {
                        <div class="col-md-4 col-6 col-sm-6">
                            <div class="form-group">
                                <label>U. Recibidas Tapa</label>
                                <input class="form-control" id="txtUnidadesRecibidasTapa" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                            </div>
                        </div>
                        <div class="col-md-4 col-6 col-sm-6">
                            <div class="form-group">
                                <label>Sobrantes Tapa</label>
                                <input class="form-control" id="txtSobrantesTapa" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                            </div>
                        </div>
                        <div class="col-md-4 col-6 col-sm-6">
                            <div class="form-group">
                                <label>U. Producidas Tapa</label>
                                <input class="form-control" id="txtUnidadesProducidasTapa" min="0" type="number" name="name" value="0" onkeypress="return SoloNumeros(event,this)" />
                            </div>
                        </div>
                    }
                    <div class="col-md-4 col-6 col-sm-6">
                        <div class="form-group">
                            <label>Grs Real</label>
                            <input class="form-control" id="txtGrs" min="0" type="number" onkeypress=" return SoloDecimales(event,this)" name="name" value="0" />
                        </div>
                    </div>
                    <div class="col-md-12 col-12 col-sm-12">
                        <div class="form-group">
                            <label>Observacion</label>
                            <textarea class="form-control" cols="12" id="txtObservacion" maxlength="250" type="text" name="name" value=""></textarea>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="GenerarControlConsumo()" id="btnGenerarControlConsumo">Generar</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
            </div>
        </div>
    </div>
</div>

