@model IEnumerable<Asiservy.Automatizacion.Datos.Datos.spConsultaConsumoTiempoMuerto>

<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.HoraPara)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoraReinicio)
        </th>      
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observacion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
     
        <td>
            @Html.DisplayFor(modelItem => item.HoraPara)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HoraReinicio)
        </td>       
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observacion)
        </td>
        
    </tr>
}

</table>
