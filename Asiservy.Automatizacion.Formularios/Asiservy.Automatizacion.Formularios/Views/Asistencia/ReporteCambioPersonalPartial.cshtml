@model List<Asiservy.Automatizacion.Datos.Datos.spReporteCambioPersonal>

<table class="table-hover table-bordered table-striped table" style="cursor: pointer" id="tblDataTable">
    <thead>
        <tr>
            <th>
                Cédula
            </th>
            <th>
                Nombres
            </th>
            <th>
                Centro de Costo Origen
            </th>
            <th>
                Recurso Origen
            </th>
            <th>
                Linea Origen
            </th>
            <th>
                Cargo Origen
            </th>
            <th>
                Centro de Costo Movido
            </th>
            <th>
                Recurso Movido
            </th>
            <th>
                Línea Movido
            </th>
            <th>
                Cargo Movido
            </th>
            <th>
                Fecha Movimiento
            </th>
            <th>
                Hora Movimietno
            </th>
            <th>Fecha Regreso</th>
            <th>Hora Regreso</th>
            <th>Usuario Creación</th>
            <th>Usuario Modificación</th>
            <th>Fecha Creación</th>
            <th>Fecha Modificación</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.CEDULA
                </td>
                <td>
                    @item.NOMBRES
                </td>
                <td>
                    @item.CentroCostosOrigen
                </td>
                <td>
                    @item.RecursoOrigen
                </td>
                <td>
                    @item.LineaOrigen
                </td>
                <td>
                    @item.CargoOrigen
                </td>
                <td>
                    @item.CentroCostoMovido
                </td>
                <td>
                    @item.RecursoMovido
                </td>
                <td>
                    @item.LineaMovido
                </td>
                <td>
                    @item.CargoMovido
                </td>
                <td>
                    @if (item.FechaMovido != null)
                    {
                        @item.FechaMovido.Value.ToString("yyyy/MM/dd");
                    }

                </td>
                <td>
                    @item.HoraMovido
                </td>
                <td>
                    @if (item.FechaRegreso != null){
                        @item.FechaRegreso.Value.ToString("yyyy/MM/dd");
                    }
                    
                </td>
                <td>
                    @item.HoraRegreso
                </td>
                <td>@item.UsuarioIngresoLog</td>
                <td>@item.UsuarioModificacionLog</td>
                <td>@item.FechaIngresoLog</td>
                <td>@item.FechaModificacionLog</td>
            </tr>
        }
    </tbody>
</table>
<script src="~/Scripts/General/dataTableJS.js"></script> 