
@{
    ViewBag.Title = "ReporteAuditoriaSangre";
}

<h4>Reporte Auditoria Sangre</h4>
<div class="row">
    <div class="col-md-3 col-sm-12">

        <div class="form-group">
            <label>Fecha:</label>
            <input type="date" id="Fecha" class="form-control" />
        </div>


    </div>
    <div class=" col-md-3 col-sm-12">

        <div class="form-group">

            <label>Línea:</label>
            @Html.DropDownList("Lineas", (IEnumerable<SelectListItem>)ViewBag.Lineas, "Selecione", new { @class = "form-control" })

        </div>


    </div>
</div>
<div class="row">
    <div class=" col-md-6 col-sm-12">
        <center>
            <div class="form-group">
                <input type="button" value="Consultar" class="btn btn-primary" id="ConsultarRptAuditoriaSangre" onclick="ConsultarReporteAuditoriaSangre()" />
            </div>
        </center>
    </div>
</div>
<div id="DivReporteSangre" class="table-responsive">


</div>
