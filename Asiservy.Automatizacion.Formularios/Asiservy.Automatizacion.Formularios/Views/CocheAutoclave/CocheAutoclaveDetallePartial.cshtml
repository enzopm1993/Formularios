@model IEnumerable<Asiservy.Automatizacion.Datos.Datos.spConsultaCocheAutoclaveDetalle>
@using Newtonsoft.Json;
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tarjeta)
        </th>
        <th>
            Hora Inicio
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Coche)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LineaProduccion)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tarjeta)
        </td>
        <td>
            @item.HoraInicio.ToString("yyyy-MM-dd HH:mm")
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Coche)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LineaProduccion)
        </td>
        <td>
            @{string json = JsonConvert.SerializeObject(item); }
            <button class="btn btn-link" onclick="EditarCocheAutoclaveDetalle(@json)">Editar</button>
            |
            <button class="btn btn-link" onclick="EliminarCocheAutoclaveDetalle(@json)">Eliminar</button>
        </td>
    </tr>
    }

</table>
