@model IEnumerable<Asiservy.Automatizacion.Datos.Datos.CC_CALIBRACION_FLUOROMETRO_DET>
@using Newtonsoft.Json;
<table class="table table-bordered table-striped" style="font-size:12px" id="tblDataTable">
    <thead>
        <tr>
            <th>
                COEFICIENTE TETERMINACION
            </th>
            <th>
                ESTANDAR
            </th>
            <th>
                VALOR ESTANDAR
            </th>
            <th>
                HORA
            </th>
            <th>USUARIO CREACION</th>
            <th>USUARIO MODIFICACION</th>
            <th>ACCIONES</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.CoeficienteDeterminacion
                </td>
                <td>
                    @item.IdEstandar
                </td>
                <td>
                    @item.ValorEstandar
                </td>
                <td>
                    @item.Hora
                </td>
                <td>
                    @item.UsuarioIngresoLog
                </td>
                <td>
                    @item.UsuarioModificacionLog
                </td>
                <td>
                <td>
                    @{string json = JsonConvert.SerializeObject(item);
                    }
                    <button id="btnActualizar" class="btn btn-link" onclick="ActulizarDetalle(@json)">Editar</button>
                    <button id="btnEliminar" class="btn btn-link" onclick="EliminarConfirmarDetalle(@json)">Eliminar</button>
                </td>
            </tr>
        }
    </tbody>
</table>

<script src="~/Scripts/General/dataTableJS.js"></script>
