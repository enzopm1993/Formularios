@model IEnumerable<Asiservy.Automatizacion.Datos.Datos.CC_ANALISIS_QUIMICO_PRODUCTO_SEMIELABORADO_DETALLE>
@using Newtonsoft.Json

<table class="table table-striped table-bordered table-hover" id="TableDetalle">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.OrdenFabricacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Proveedor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Especie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Talla)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cliente)
            </th>

        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            var json = JsonConvert.SerializeObject(new {item.Cliente,item.Especie,item.IdCabeceraAnalisisQuimicoProductoSe,item.IdDetalleAnalisisQuimicoProductoSe,item.Lote
            ,item.OrdenFabricacion,item.Proveedor,item.Talla});
            <tr style="cursor:pointer" onclick="VerSubDetalle(@json)">

                <td>
                    @Html.DisplayFor(modelItem => item.OrdenFabricacion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Proveedor)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Lote)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Especie)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Talla)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cliente)
                </td>


            </tr>
        }
    </tbody>

</table>
